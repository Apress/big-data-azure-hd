<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="ConsumerKey" value="{YourAppComsumerKey}" />
    <add key="ConsumerSecret" value="{YourAppConsumerSecret}" />
    <add key="AccessToken" value="{YourAppAccessToken}" />
    <add key="AccessTokenSecret" value="{YourAppAccessTokenSecret}" />

    <add key="SqlAzureConnectionString" value="{Azure Sql Database Connection String}" />
    <add key="SqlAzureTableName" value="Tweets" />
    <add key="SqlAzureTableColumns" value="TweetId,Text,RetweetCount,FavoriteCount,UserFollowerCount,Score,TimeStamp" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="9.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
